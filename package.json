{
  "name": "create-react-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run docgen && PORT=8002 react-app-rewired start",
    "serve:ip": "env-cmd -f .env.ip npm run serve",
    "serve:te": "env-cmd -f .env.te npm run serve",
    "serve:ie": "env-cmd -f .env.ie npm run serve",
    "serve:se": "env-cmd -f .env.se npm run serve",
    "serve:pe": "env-cmd -f .env.pe npm run serve",
    "build": "npm run docgen && env-cmd -f .env.pe react-app-rewired build",
    "build:te": "npm run docgen && env-cmd -f .env.te react-app-rewired build",
    "build:ie": "npm run docgen && env-cmd -f .env.ie react-app-rewired build",
    "build:se": "npm run docgen && env-cmd -f .env.se react-app-rewired build",
    "build:pe": "npm run docgen && env-cmd -f .env.pe react-app-rewired build",
    "docgen": "docgen-react --watch=false",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "fix": "npm run lint -- --fix",
    "stylelint": "stylelint --fix src/common/components/DemoPage/Map.module.less"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@ant-design/icons": "4.8.1",
    "@dnd-kit/core": "^6.0.7",
    "@dnd-kit/sortable": "^7.0.2",
    "@lhb/cache": "^2.0.1",
    "@lhb/func": "^2.0.0",
    "@lhb/hook": "^1.0.0",
    "@lhb/regexp": "^2.0.0",
    "@reduxjs/toolkit": "1.7.1",
    "@sentry/react": "^7.44.2",
    "@sentry/tracing": "^7.44.2",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-react": "^1.0.6",
    "antd": "^4.24.8",
    "array-move": "^4.0.0",
    "axios": "0.24.0",
    "benz-amr-recorder": "1.1.5",
    "core-js": "3.20.2",
    "dayjs": "1.10.7",
    "echarts": "^5.4.3",
    "flow-engine": "^1.3.9",
    "html2canvas": "^1.4.1",
    "immutability-helper": "^3.1.1",
    "qs": "6.10.3",
    "react-activation": "^0.11.2",
    "react-app-polyfill": "3.0.0",
    "react-color": "2.19.3",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-draft-wysiwyg": "^1.15.0",
    "react-easy-crop": "^4.7.1",
    "react-full-screen": "1.1.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-resizable": "^3.0.5",
    "react-sortable-hoc": "^2.0.0",
    "react-sticky": "^6.0.3",
    "react-transition-group": "4.4.2",
    "resize-observer-polyfill": "^1.5.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@lhb/component-docgen-react": "^1.1.4",
    "@types/node": "16.11.19",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.14",
    "@types/react-sticky": "^6.0.4",
    "@types/react-transition-group": "4.4.4",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "antd-dayjs-webpack-plugin": "1.0.6",
    "babel-plugin-import": "1.13.3",
    "customize-cra": "1.0.0",
    "customize-cra-less-loader": "2.0.0",
    "env-cmd": "10.1.0",
    "http-proxy-middleware": "2.0.1",
    "husky": "1.3.1",
    "less": "4.1.2",
    "less-loader": "10.2.0",
    "lint-staged": "12.1.7",
    "progress-bar-webpack-plugin": "2.1.0",
    "react": "18.2.0",
    "react-app-rewired": "2.1.11",
    "react-dom": "^18.2.0",
    "react-redux": "7.2.6",
    "react-router-dom": "6.2.1",
    "react-scripts": "5.0.0",
    "tslib": "^2.5.0",
    "typescript": "4.5.4",
    "validate-commit-msg": "2.14.0",
    "video.js": "^7.20.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "validate-commit-msg"
    }
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie >= 9"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie >= 9"
    ]
  },
  "resolutions": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.14"
  }
}
