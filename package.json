{
  "name": "create-react-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run docgen && env-cmd -f .env react-app-rewired start",
    "serve:ie": "npm run docgen && env-cmd -f .env.ie react-app-rewired start ",
    "serve:te": "npm run docgen && env-cmd -f .env.te react-app-rewired start ",
    "serve:se": "npm run docgen && env-cmd -f .env.se react-app-rewired start ",
    "serve:pe": "npm run docgen && env-cmd -f .env.pe react-app-rewired start ",
    "build": "npm run docgen && env-cmd -f .env.pe react-app-rewired build",
    "build:ie": "npm run docgen && env-cmd -f .env.ie react-app-rewired build ",
    "build:te": "npm run docgen && env-cmd -f .env.te react-app-rewired build ",
    "build:se": "npm run docgen && env-cmd -f .env.se react-app-rewired build ",
    "build:pe": "npm run docgen && env-cmd -f .env.pe react-app-rewired build ",
    "docgen": "docgen-react --watch=false"
  },
  "dependencies": {
