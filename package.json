    "react-virtualized-auto-sizer": "1.0.21",
    "react-window": "1.8.10",
    "react-window-infinite-loader": "1.0.9",
    "resize-observer-polyfill": "^1.5.1",
    "uuid": "^9.0.1",
    "video.js": "^7.20.3"
