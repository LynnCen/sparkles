        "form-data": "^3.0.0",
        "get-prefix": "^1.0.0",
        "get-stream": "^6.0.1",
        "got": "^11.8.2",
        "han": "^0.0.7",
        "i18n": "^0.8.3",
        "iconv-lite": "^0.6.2",
        "image-size": "^1.0.0",
        "intl": "^1.2.5",
        "jr-qrcode": "^1.1.4",
        "less-loader": "^6.2.0",
        "lodash": "^4.17.21",
        "long": "^4.0.0",
        "md5": "^2.3.0",
        "md5-node": "^1.0.1",
        "mime-types": "^2.1.30",
        "mobx": "^5.0.3",
        "mobx-react": "^5.2.3",
        "moment": "^2.27.0",
        "osenv": "^0.1.5",
        "pinyin": "^2.10.2",
        "play-sound": "^1.1.1",
