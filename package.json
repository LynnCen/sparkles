        "btoa": "^1.2.1",
        "chalk": "^4.1.0",
        "classnames": "^2.3.1",
        "compressorjs": "^1.0.7",
        "delegate": "^3.1.3",
        "detectrtc": "^1.3.9",
        "dexie": "^3.0.3",
        "dmg-builder": "^21.2.0",
        "draft-js": "^0.11.7",
